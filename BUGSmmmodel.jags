    for(i in 1:n.observations){
    mean[i]<- (a*day[i])/(b+day[i])  #ricker function relationship
    rate[i]<- shape/mean[i]            # BUGS wants rate not scale
    titer[i] ~ dgamma(shape,rate[i])
    }
    
